{
  "name": "prisma-generator-proto-monorepo",
  "version": "0.0.0",
  "description": "Monorepo for @prisma-generator-proto",
  "private": true,
  "workspaces": [
    "example",
    "packages/*"
  ],
  "devDependencies": {
    "turbo": "^1.0.28"
  },
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"]
      },
      "dev": {
        "dependsOn": ["^build"],
        "cache": false
      }
    }
  }
}
